<extension>
    <modules>
    
        <!--
            Customization modules for adding i18n labels and importing client-side dependencies for
            the web previewer plugins
        -->
        <module>
            <id>STL Media Viewer</id>
            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.components.preview</targetPackageRoot>
                    <dependencies>
                        <css>/res/stl_media_viewer/stlViewer.css</css>
                        <js>/res/stl_media_viewer/jsc3d.js</js>
                        <js>/res/stl_media_viewer/jsc3d.console.js</js>
                        <js>/res/stl_media_viewer/stlViewer.js</js>
                    </dependencies>
                    <sourcePackageRoot/>
                </customization>
            </customizations>
            <auto-deploy>true</auto-deploy>
        </module>
      
    </modules>
</extension>